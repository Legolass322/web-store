{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": [
            "error", 
            { 
                "vars": "all", 
                "args": "none", 
                "ignoreRestSiblings": false 
            }
        ],
        "semi": ["error", "never"],
        "quotes": ["warn", "single"]
    }
}
